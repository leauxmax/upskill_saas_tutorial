<div class="container", style="max-width: 100%">
  <div class="row", style="max-width: 100%">
    <div class="col-md-8 col-md-offset-2", style="max-width: 100%">
      <ul class="list-unstyled", style="max-width: 100%">
        <% @users.each do |user| %>
          <% if user.profile %> 
          <li>
            <div class="well row <%= cycle('white-bg', '') %> ">
              <div class="col-sm-4 text-center">
                <% if user.profile.avatar %>
                  <div class="disco">
                  </div>
                  <%= link_to user do %>
                    <%= image_tag user.profile.avatar.url, class: 'user-index-avatar' %>
                  <% end %>
                <% end %>
                
              </div>
              <div>
                <%= link_to user do %>
                  <h2 style="font-size: 240%; color: #88B04B!important"><%= user.profile.first_name %><%= user.profile.last_name %></h2>
                    
                <% end %> 
                  
                  <!--<p>-->
                  <!--  <%= user.profile.job_title %>-->
                  <!--  <% if user.plan_id == 2 %>-->
                  <!--    <span class="label label-success">PRO</span>-->
                  <!--  <% end %>-->
                  <!--</p>-->
              </div>
            </div>
          </li>
          <% end %>
        <% end %>
      </ul>
    </div>
  </div>
</div>
